{
  "editor.fontSize": 18,
  "window.zoomLevel": 1,
  "code-runner.executorMap": {
    "typescript": "npx ts-node --loader ts-node/esm --files --transpile-only"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["typescript"],
  "liveServer.settings.port": 5501
}
